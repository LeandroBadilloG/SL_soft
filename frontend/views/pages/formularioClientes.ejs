    <div class="container mt-4">
        <div class="row">
            <div class="col-md-6">
                <h1 class="text-center mt-4">Registro de Cliente</h1>
                <form class="m-5" action="/v1/registrarCliente" method="post">
                    <div class="form-group ">
                        <label for="nombre" class="form-label">NOMBRE</label>
                        <input type="text" class="form-control" name="nombre" required placeholder="Juan Camilo">

                        <label for="apellido" class="form-label mt-2">APELLIDO</label>
                        <input type="text" class="form-control" name="apellido" required placeholder="Martinez Mora">

                        <label for="documento" class="form-label mt-2">DOCUMENTO</label>
                        <input type="number" class="form-control" name="documento" required>

                        <label for="celular" class="form-label mt-2">CELULAR</label>
                        <input type="number" class="form-control" name="celular" required>

                        <label for="email" class="form-label mt-2">CORREO</label>
                        <input type="email" class="form-control" name="email" required
                            placeholder="juanmartinez@gmail.com">

                        <label for="direccion" class="form-label mt-2">DIRECCIÓN</label>
                        <input type="text" class="form-control" name="direccion" required
                            placeholder="Cll 65 # 65 - 54 Itagüi">

                        <label for="password" class="form-label mt-2">CONTRASEÑA</label>
                        <input type="password" class="form-control" name="password" required>
                    </div>
                    <div class="form-group text-center ">
                        <input type="submit" class="btn btn-primary p-1 mt-5" value="Registrar">
                    </div>
                </form>
            </div>
